    <servlet-name>PDFLoader</servlet-name>
    <url-pattern>/PDFLoader</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Calendar</servlet-name>
    <servlet-class>myApp.server.frm.Calendar</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Calendar</servlet-name>
    <url-pattern>/Calendar</url-pattern>
  </servlet-mapping>
  

